{
  "name": "annalise-ai-takehome",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "create-migration": "npx mikro-orm migration:create --path src/migrations",
    "migrate-up": "npx mikro-orm migration:up",
    "drop-and-migrate-up": "npx mikro-orm migration:fresh",
    "test": "npx mocha --timeout 5000 --exit -r ts-node/register tests/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DerekWang98/annalise-ai-takehome.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DerekWang98/annalise-ai-takehome/issues"
  },
  "homepage": "https://github.com/DerekWang98/annalise-ai-takehome#readme",
  "dependencies": {
    "@mikro-orm/cli": "5.3.0",
    "@mikro-orm/core": "5.3.0",
    "@mikro-orm/migrations": "^5.3.0",
    "@mikro-orm/postgresql": "5.3.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "9.1.1",
    "chai": "4.3.6",
    "dotenv": "16.0.x",
    "express": "4.18.x",
    "express-promise-router": "^4.1.1",
    "joi": "17.6.0",
    "mocha": "10.0.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/node": "18.6.3",
    "@types/supertest": "2.0.12",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "supertest": "6.2.4",
    "ts-node": "10.9.1"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
